: {\n    n: parseInt(localStorage.getItem(\"n\"))\n  }\n};\n\n// 所有和视图相关的都放到V里面\nconst v = {\n    el:null,\n  html: `\n<div class=\"output\">\n  <span id=\"number\">{{n}}</span>\n</div>\n<div id=\"actions\">\n  <button id=\"add1\">+1</button>\n  <button id=\"minus2\">-1</button>\n  <button id=\"mul2\">*2</button>\n  <button id=\"divide2\">÷2</button>\n</div>\n`,\n  render(container) {\n      if(v.el === null){\n          v.el = $(v.html.replace(`{{n}}`,m.data.n)).appendTo($(container))\n      }else{\n          const newEl = $(v.html.replace(`{{n}}`,m.data.n))\n          v.el.replaceWith(newEl)\n          v.el = newEl;\n      }\n  }\n};\n\n//其他部分都放到C里面\nconst c = {\n  init(container) {\n      v.render(container)\n    c.ui = {\n      button1: $(\"#add1\"),\n      button2: $(\"#minus2\"),\n      button3: $(\"#mul2\"),\n      button4: $(\"#divide2\"),\n      number: $(\"#number\")\n    };\n    c.bindEvents();\n  },\n  bindEvents() {\n    c.ui.button1.on(\"click\", () => {\n        let n = parseInt(c.ui.number.text());\n        n = n + 1;\n        localStorage.setItem(\"n\", n);\n        c.ui.number.text(n);\n    });\n    c.ui.button2.on(\"click\", () => {\n        let n = parseInt(c.ui.number.text());\n        n = n - 1;\n        localStorage.setItem(\"n\", n);\n        c.ui.number.text(n);\n    });\n    c.ui.button3.on(\"click\", () => {\n      let n = parseInt(c.ui.number.text());\n      n = n * 2;\n      localStorage.setItem(\"n\", n);\n      c.ui.number.text(n);\n    });\n    c.ui.button4.on(\"click\", () => {\n      let n = parseInt(c.ui.number.text());\n      n = n / 2;\n      localStorage.setItem(\"n\", n);\n      c.ui.number.text(n);\n    });\n  }\n};\n\nexport default c;"},"lineCount":null}},"error":null,"hash":"8aa262ec5af56800552df4b9f31e5a6f","cacheData":{"env":{}}}